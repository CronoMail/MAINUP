{
    "functions": {
      "api/upload": {
        "maxDuration": 60,
        "memory": 1024
      }
    },
    "routes": [
      {
        "src": "/api/upload",
        "dest": "/api/upload",
        "methods": ["POST"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Content-Type": "application/json",
          "max-http-header-size": "32768",
          "max-http-buffer-size": "10485760"
        }
      }
    ]
}