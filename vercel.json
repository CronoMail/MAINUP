
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "assets/**/*", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
 {
  "git": {
    "deploymentEnabled": {
      "imageUpload": false
    }
  },
  "ignoreCommand": "git log -1 --pretty=%B | grep -E '\\[skip deploy\\]|\\[ci skip\\]'"
}
